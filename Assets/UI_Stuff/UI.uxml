                <ui:VisualElement name="Buttons" style="height: 15%; width: 100%; flex-direction: row; align-items: center; justify-content: space-between; padding-left: -4px; display: flex; margin-bottom: 1%;">
                    <ui:Button text="Settings" display-tooltip-when-elided="true" name="settingsButton" focusable="false" class="buttonStyle" style="background-color: rgba(188, 188, 188, 0); left: auto; width: 25%; height: 85%; font-size: 48%;" />
                    <ui:Button text="Unpause" display-tooltip-when-elided="true" name="resumeButton" focusable="false" class="buttonStyle" style="left: auto; position: relative; width: 25%; height: 85%; font-size: 48%;" />
                    <ui:Button text="Main menu" display-tooltip-when-elided="true" name="menuButton" focusable="false" class="buttonStyle" style="left: auto; height: 85%; width: 25%; font-size: 48%;" />
                </ui:VisualElement>
                <ui:VisualElement name="Utility" style="max-width: none; height: 100%; width: 100%; flex-direction: row; align-items: stretch; justify-content: space-between; display: flex;">
                    <ui:VisualElement name="Items" style="height: 100%; width: 50%; padding-left: 0; padding-right: 36px; padding-top: 0; padding-bottom: 0;" />
                    <ui:VisualElement name="Map" style="height: 100%; width: 50%; background-image: url(&apos;/Assets/UI_Stuff/mapContainer.png&apos;);" />
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:Button display-tooltip-when-elided="true" name="secretButton" style="background-color: rgba(188, 188, 188, 0); border-left-color: rgba(149, 149, 149, 0); border-right-color: rgba(149, 149, 149, 0); border-top-color: rgba(149, 149, 149, 0); border-bottom-color: rgba(149, 149, 149, 0); position: absolute; top: 10%; left: 49%; width: 2%; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; height: 2%; overflow: visible;" />
