                <ui:Button text="&#10;" display-tooltip-when-elided="true" name="healButton" class="buttonStyle" style="left: 0; font-size: 32px;" />
            </ui:VisualElement>
            <ui:VisualElement name="godmode" style="flex-direction: row; display: none;">
