                <ui:Button text="&#10;" display-tooltip-when-elided="true" name="healButton" focusable="false" class="buttonStyle" style="left: auto; font-size: 32px; width: 50%; height: 78%; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;" />
