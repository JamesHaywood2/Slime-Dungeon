            <ui:VisualElement name="hpFill" style="height: 100%; width: 100%; background-color: rgb(204, 0, 4); border-left-color: rgba(0, 0, 0, 0); border-right-color: rgba(0, 0, 0, 0); border-top-color: rgba(0, 0, 0, 0); border-bottom-color: rgba(0, 0, 0, 0); display: flex;" />
        </ui:VisualElement>
        <ui:Label text="HP: current/max" display-tooltip-when-elided="true" name="hpExact" style="font-size: 35%; -unity-text-outline-color: rgb(0, 0, 0); color: rgb(255, 255, 255); -unity-text-outline-width: 0.5px; -unity-font-style: bold; height: 46px; width: auto; opacity: 1; margin-left: 15px;" />
    </ui:VisualElement>
    <ui:VisualElement name="pauseMenu" style="height: 100%; width: 100%; background-color: rgba(43, 43, 43, 0.52); align-items: center; justify-content: flex-start; flex-direction: column; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-bottom: 0; display: none; padding-left: 0; padding-right: 0; padding-top: 0;">
        <ui:Label text="Game Paused" display-tooltip-when-elided="true" name="pauseText" style="font-size: 59%; -unity-font-style: bold; color: rgb(0, 0, 0); -unity-text-outline-color: rgb(255, 255, 255); -unity-text-outline-width: initial; -unity-text-align: middle-center; align-items: center; width: auto; white-space: normal; position: relative; bottom: auto; left: auto; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; top: 8%;" />
        <ui:VisualElement name="MenuContainer" style="height: 100%; width: 100%; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; background-color: rgba(0, 0, 0, 0); left: 0; top: 0; align-items: center; position: absolute; right: 0; bottom: 0; padding-left: 0;">
